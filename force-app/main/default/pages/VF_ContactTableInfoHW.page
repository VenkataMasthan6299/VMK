<apex:page standardController="Contact" recordSetVar="accts">
    <apex:pageblock >
        <apex:pageBlockSection title="Table Information" columns="1">
          <apex:pageBlockTable value="{!accts}" var="acc">
              <apex:column value="{!acc.FirstName}"/>
              <apex:column value="{!acc.LastName}"/>
              <apex:column value="{!acc.Email}"/>
              <apex:column value="{!acc.MobilePhone}"/>
          </apex:pageBlockTable> 
        </apex:pageBlockSection>
    </apex:pageblock>
</apex:page>